Algoritmo "lermatriz"

Var
   M: vetor[1..3, 1..2] de inteiro
   L, C: inteiro



Inicio
   Para L <- 1 ate 3 faca
      Para C <- 1 ate 2 faca
         Escreval ("Digite o valor da posição ", L, ",", C)
         Leia (M[L,C])
      FimPara
   FimPara
   Para L <- 1 ate 3 faca
      Para C<- 1 ate 2 faca
         Escreva (M[L, C])
      FimPara
      Escreval()
   FimPara



Fimalgoritmo