Algoritmo "area do circulo"
Var
   p, a: real
   R: inteiro

Inicio
      Escreval ("-----------------")
      Escreval (" ÁREA DO CÍRCULO")
      Escreval ("-----------------")
      Escreva ("Digite o valor de pi:")
      leia (p)
      Escreva ("Digite o valor de do Raio do círculo:")
      leia (R)
      a := p * R^2
      Escreva("O valor da área do círculo é igual a: ", a)
Fimalgoritmo