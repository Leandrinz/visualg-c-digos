Algoritmo "conversao Dolar"

Var
   cd, D, R : real
Inicio
      Escreval ("Digite a cotação do dólar: ")
      leia (cd)
      Escreval ("Digite o valor (em real) que você quer converter para dolar: ")
      leia(R)
      D := (R/cd)
      Escreval ("Após o valor ser convertido, o resultado em dólar é de: ", D)
Fimalgoritmo