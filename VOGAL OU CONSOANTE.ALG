algoritmo "vogal ou consoante"
var
   l: caractere
inicio
      escreval ("Digite uma letra")
      leia(l)
      se (l = "a") ou (l="e") ou (l="i") ou (l="o") ou (l = "u") entao
         Escreval ("A letra digitada é uma vogal")
      senao
           Escreval ("A letra digitada é consoante")
      fimse

fimalgoritmo