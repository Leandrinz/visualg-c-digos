Algoritmo "Conversor"

Var
   R, D, C, N : Real

Inicio
      Escreval ("Quantas converções você quer fazer? ")
      Leia (N)
      C <- 1
      Enquanto (C <= N) faca
         Escreval ("Quantos reias você quer converter para dolar? ")
         Leia (R)
         D <- R/6
         Escreval (D," U$")
         C <- C + 1
      FimEnquanto
      


Fimalgoritmo